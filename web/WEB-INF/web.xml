<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>servlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayDoctors</servlet-name>
        <servlet-class>servlet.DisplayDoctors</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PatientRegistration</servlet-name>
        <servlet-class>servlet.PatientRegistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TimeClock</servlet-name>
        <servlet-class>servlet.TimeClock</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoctorMenu</servlet-name>
        <servlet-class>servlet.DoctorMenu</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayDoctors</servlet-name>
        <url-pattern>/DisplayDoctors</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TimeClock</servlet-name>
        <url-pattern>/TimeClock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatientRegistration</servlet-name>
        <url-pattern>/PatientRegistration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoctorMenu</servlet-name>
        <url-pattern>/DoctorMenu</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
